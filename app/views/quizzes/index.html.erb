<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>Quiz name</th>
      <th>Owner</th>
      <th>Quiz detail</th>
    </tr>
  </thead>
  <tbody>
    <% @quiz.each do | quiz | %>
    <tr>
      <td><%= quiz.quiz_name%></td>
      <td><%= quiz.user.full_name%></td>
      <td><%= link_to "Detail", quiz_path(quiz.id), { :class => "btn btn-sm btn-default" } %></td>
    </tr>
    <% end %>
  </tbody>
</table>